{"ast":null,"code":"import MiniStatisticsCard from \"@/examples/Cards/MiniStatisticsCard.vue\";\nimport GradientLineChart from \"@/examples/Charts/GradientLineChart.vue\";\nimport Carousel from \"./components/Carousel.vue\";\nimport CategoriesList from \"./components/CategoriesList.vue\";\nimport US from \"@/assets/img/icons/flags/US.png\";\nimport DE from \"@/assets/img/icons/flags/DE.png\";\nimport GB from \"@/assets/img/icons/flags/GB.png\";\nimport BR from \"@/assets/img/icons/flags/BR.png\";\nexport default {\n  __name: 'Dashboard',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const sales = {\n      us: {\n        country: \"Ahmed Attia\",\n        sales: \"C123\",\n        value: \"High\",\n        bounce: \"Pending\",\n        flag: US\n      },\n      germany: {\n        country: \"Ranim Mehri\",\n        sales: \"C345\",\n        value: \"Low\",\n        bounce: \"Unsolved\",\n        flag: DE\n      },\n      britain: {\n        country: \"Eya Thebty\",\n        sales: \"C256\",\n        value: \"Medium\",\n        bounce: \"Solved\",\n        flag: GB\n      },\n      brasil: {\n        country: \"Helmy Chelly\",\n        sales: \"C562\",\n        value: \"Low\",\n        bounce: \"Solved\",\n        flag: BR\n      }\n    };\n    const __returned__ = {\n      sales,\n      MiniStatisticsCard,\n      GradientLineChart,\n      Carousel,\n      CategoriesList,\n      get US() {\n        return US;\n      },\n      get DE() {\n        return DE;\n      },\n      get GB() {\n        return GB;\n      },\n      get BR() {\n        return BR;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["MiniStatisticsCard","GradientLineChart","Carousel","CategoriesList","US","DE","GB","BR","sales","us","country","value","bounce","flag","germany","britain","brasil"],"sources":["/home/ahmed/Downloads/vue-argon-dashboard-main/src/views/Dashboard.vue"],"sourcesContent":["<script setup>\nimport MiniStatisticsCard from \"@/examples/Cards/MiniStatisticsCard.vue\";\nimport GradientLineChart from \"@/examples/Charts/GradientLineChart.vue\";\nimport Carousel from \"./components/Carousel.vue\";\nimport CategoriesList from \"./components/CategoriesList.vue\";\n\nimport US from \"@/assets/img/icons/flags/US.png\";\nimport DE from \"@/assets/img/icons/flags/DE.png\";\nimport GB from \"@/assets/img/icons/flags/GB.png\";\nimport BR from \"@/assets/img/icons/flags/BR.png\";\n\nconst sales = {\n  us: {\n    country: \"Ahmed Attia\",\n    sales: \"C123\",\n    value: \"High\",\n    bounce: \"Pending\",\n    flag: US,\n  },\n  germany: {\n    country: \"Ranim Mehri\",\n    sales: \"C345\",\n    value: \"Low\",\n    bounce: \"Unsolved\",\n    flag: DE,\n  },\n  britain: {\n    country: \"Eya Thebty\",\n    sales: \"C256\",\n    value: \"Medium\",\n    bounce: \"Solved\",\n    flag: GB,\n  },\n  brasil: {\n    country: \"Helmy Chelly\",\n    sales: \"C562\",\n    value: \"Low\",\n    bounce: \"Solved\",\n    flag: BR,\n  },\n};\n</script>\n<template>\n  <div class=\"py-4 container-fluid\">\n    <div class=\"row\">\n      <div class=\"col-lg-12\">\n        <div class=\"row\">\n          <div class=\"col-lg-3 col-md-6 col-12\">\n            <mini-statistics-card\n              title=\"Total complaints\"\n              value=\"53\"\n              description=\"<span\n                class='text-sm font-weight-bolder text-success'\n                >+5%</span> since yesterday\"\n              :icon=\"{\n                component: 'ni ni-chat-round',\n                background: 'bg-gradient-primary',\n                shape: 'rounded-circle',\n              }\"\n            />\n          </div>\n          <div class=\"col-lg-3 col-md-6 col-12\">\n            <mini-statistics-card\n              title=\"Today's complaints\"\n              value=\"20\"\n              description=\"<span\n                class='text-sm font-weight-bolder text-success'\n                >+3%</span> since last week\"\n              :icon=\"{\n                component: 'ni ni-notification-70',\n                background: 'bg-gradient-info',\n                shape: 'rounded-circle',\n              }\"\n            />\n          </div>\n          <div class=\"col-lg-3 col-md-6 col-12\">\n            <mini-statistics-card\n              title=\"solved complaints\"\n              value=\"30\"\n              description=\"<span\n                class='text-sm font-weight-bolder text-danger'\n                >-2%</span> since last quarter\"\n              :icon=\"{\n                component: 'ni ni-check-bold',\n                background: 'bg-gradient-success',\n                shape: 'rounded-circle',\n              }\"\n            />\n          </div>\n          <div class=\"col-lg-3 col-md-6 col-12\">\n            <mini-statistics-card\n              title=\"unsolved\"\n              value=\"10\"\n              description=\"<span\n                class='text-sm font-weight-bolder text-success'\n                >+5%</span> than last month\"\n              :icon=\"{\n                component: 'ni ni-fat-remove',\n                background: 'bg-gradient-danger',\n                shape: 'rounded-circle',\n              }\"\n            />\n          </div>\n        </div>\n        <div class=\"row\">\n          <div class=\"col-lg-7 mb-lg\">\n            <!-- line chart -->\n            <div class=\"card z-index-2\">\n              <gradient-line-chart\n                id=\"chart-line\"\n                title=\"Complaints Analysis\"\n                description=\"<i class='fa fa-arrow-up text-success'></i>\n      <span class='font-weight-bold'>4% more</span> in 2024\"\n                :chart=\"{\n                  labels: [\n                    'Apr',\n                    'May',\n                    'Jun',\n                    'Jul',\n                    'Aug',\n                    'Sep',\n                    'Oct',\n                    'Nov',\n                    'Dec',\n                  ],\n                  datasets: [\n                    {\n                      label: 'Mobile Apps',\n                      data: [50, 40, 300, 220, 500, 250, 400, 230, 500],\n                    },\n                  ],\n                }\"\n              />\n            </div>\n          </div>\n          <div class=\"col-lg-5\">\n            <carousel />\n          </div>\n        </div>\n        <div class=\"row mt-4\">\n          <div class=\"col-lg-7 mb-lg-0 mb-4\">\n            <div class=\"card\">\n              <div class=\"p-3 pb-0 card-header\">\n                <div class=\"d-flex justify-content-between\">\n                  <h6 class=\"mb-2\">Complaints by</h6>\n                </div>\n              </div>\n              <div class=\"table-responsive\">\n                <table class=\"table align-items-center\">\n                  <tbody>\n                    <tr v-for=\"(sale, index) in sales\" :key=\"index\">\n                      <td class=\"w-30\">\n                        <div class=\"px-2 py-1 d-flex align-items-center\">\n                          <div>\n                            <img :src=\"sale.flag\" alt=\"Country flag\" />\n                          </div>\n                          <div class=\"ms-4\">\n                            <p class=\"mb-0 text-xs font-weight-bold\">\n                              Client name:\n                            </p>\n                            <h6 class=\"mb-0 text-sm\">{{ sale.country }}</h6>\n                          </div>\n                        </div>\n                      </td>\n                      <td>\n                        <div class=\"text-center\">\n                          <p class=\"mb-0 text-xs font-weight-bold\">Id</p>\n                          <h6 class=\"mb-0 text-sm\">{{ sale.sales }}</h6>\n                        </div>\n                      </td>\n                      <td>\n                        <div class=\"text-center\">\n                          <p class=\"mb-0 text-xs font-weight-bold\">Priority:</p>\n                          <h6 class=\"mb-0 text-sm\">{{ sale.value }}</h6>\n                        </div>\n                      </td>\n                      <td class=\"text-sm align-middle\">\n                        <div class=\"text-center col\">\n                          <p class=\"mb-0 text-xs font-weight-bold\">Status:</p>\n                          <h6 class=\"mb-0 text-sm\">{{ sale.bounce }}</h6>\n                        </div>\n                      </td>\n                    </tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n          </div>\n          <div class=\"col-lg-5\">\n            <categories-list\n              :categories=\"[\n                {\n                  icon: {\n                    component: 'ni ni-laptop',\n                    background: 'dark',\n                  },\n                  label: 'Technical',\n                  description: '250 in stock <strong>346+ sold</strong>',\n                },\n                {\n                  icon: {\n                    component: 'ni ni-hat3',\n                    background: 'dark',\n                  },\n                  label: 'Educational',\n                  description: '123 closed <strong>15 open</strong>',\n                },\n                {\n                  icon: { component: 'ni ni-box-2', background: 'dark' },\n                  label: 'Commercial',\n                  description: '1 is active <strong>40 closed</strong>',\n                },\n                {\n                  icon: { component: 'ni ni-satisfied', background: 'dark' },\n                  label: 'Logistics',\n                  description: '+ 430',\n                },\n              ]\"\n            />\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n"],"mapp