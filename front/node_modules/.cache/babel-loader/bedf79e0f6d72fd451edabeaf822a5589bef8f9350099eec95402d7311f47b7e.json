{"ast":null,"code":"import { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport ArgonAvatar from \"@/components/ArgonAvatar.vue\";\nimport img1 from \"../../assets/img/logos/mastercard.png\";\nexport default {\n  __name: 'MasterCard',\n  props: {\n    card: {\n      type: Object,\n      number: String,\n      holderName: String,\n      expiryDate: String,\n      holderText: String,\n      expiryText: String,\n      background: String,\n      default: () => ({\n        number: \"4562 1122 4594 7852\",\n        holderName: \"Jack Peterson\",\n        expiryDate: \"11/22\",\n        holderText: \"Card Holder\",\n        expiryText: \"Expires\",\n        background: \"dark\"\n      })\n    }\n  },\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const isRTL = computed(() => store.state.isRTL);\n    const __returned__ = {\n      store,\n      isRTL,\n      computed,\n      get useStore() {\n        return useStore;\n      },\n      ArgonAvatar,\n      get img1() {\n        return img1;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","useStore","ArgonAvatar","img1","store","isRTL","state"],"sources":["/home/ahmed/Downloads/vue-argon-dashboard-main/src/examples/Cards/MasterCard.vue"],"sourcesContent":["<script setup>\nimport { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport ArgonAvatar from \"@/components/ArgonAvatar.vue\";\nimport img1 from \"../../assets/img/logos/mastercard.png\";\n\nconst store = useStore();\nconst isRTL = computed(() => store.state.isRTL);\ndefineProps({\n  card: {\n    type: Object,\n    number: String,\n    holderName: String,\n    expiryDate: String,\n    holderText: String,\n    expiryText: String,\n    background: String,\n    default: () => ({\n      number: \"4562 1122 4594 7852\",\n      holderName: \"Jack Peterson\",\n      expiryDate: \"11/22\",\n      holderText: \"Card Holder\",\n      expiryText: \"Expires\",\n      background: \"dark\",\n    }),\n  },\n});\n</script>\n<template>\n  <div class=\"card bg-transparent shadow-xl\">\n    <div\n      class=\"overflow-hidden position-relative border-radius-xl\"\n      :style=\"{\n        backgroundImage: 'url(' + require('@/assets/img/card-visa.jpg') + ')',\n      }\"\n    >\n      <span class=\"mask\" :class=\"`bg-gradient-${card.background}`\"></span>\n      <div class=\"card-body position-relative z-index-1 p-3\">\n        <i class=\"fas fa-wifi text-white p-2\" aria-hidden=\"true\"></i>\n        <h5 class=\"text-white mt-4 mb-5 pb-2\">\n          {{ card.number }}\n        </h5>\n        <div class=\"d-flex\">\n          <div class=\"d-flex\">\n            <div :class=\"isRTL ? 'ms-4' : 'me-4'\">\n              <p class=\"text-white text-sm opacity-8 mb-0\">\n                {{ card.holderText }}\n              </p>\n              <h6 class=\"text-white mb-0\">{{ card.holderName }}</h6>\n            </div>\n            <div>\n              <p class=\"text-white text-sm opacity-8 mb-0\">\n                {{ card.expiryText }}\n              </p>\n              <h6 class=\"text-white mb-0\">{{ card.expiryDate }}</h6>\n            </div>\n          </div>\n          <div\n            class=\"w-20 d-flex align-items-end justify-content-end\"\n            :class=\"isRTL ? 'me-auto' : 'ms-auto'\"\n          >\n            <argon-avatar class=\"w-60 mt-2\" :image=\"img1\" alt=\"logo\" />\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n"],"mappings":"AACA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,WAAW,MAAM,8BAA8B;AACtD,OAAOC,IAAI,MAAM,uCAAuC;;;;;;;;;;;;;;;;;;;;;;;;;;IAExD,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;IACxB,MAAMI,KAAK,GAAGL,QAAQ,CAAC,MAAMI,KAAK,CAACE,KAAK,CAACD,KAAK,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}