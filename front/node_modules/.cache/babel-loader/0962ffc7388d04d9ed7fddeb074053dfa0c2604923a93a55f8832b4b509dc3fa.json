{"ast":null,"code":"import { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport SidenavList from \"./SidenavList.vue\";\nimport logo from \"@/assets/img/logo-ct-dark.png\";\nimport logoWhite from \"@/assets/img/logo-ct.png\";\nexport default {\n  __name: 'index',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const isRTL = computed(() => store.state.isRTL);\n    const layout = computed(() => store.state.layout);\n    const sidebarType = computed(() => store.state.sidebarType);\n    const darkMode = computed(() => store.state.darkMode);\n    const __returned__ = {\n      store,\n      isRTL,\n      layout,\n      sidebarType,\n      darkMode,\n      computed,\n      get useStore() {\n        return useStore;\n      },\n      SidenavList,\n      get logo() {\n        return logo;\n      },\n      get logoWhite() {\n        return logoWhite;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","useStore","SidenavList","logo","logoWhite","store","isRTL","state","layout","sidebarType","darkMode"],"sources":["/home/ahmed/Downloads/vue-argon-dashboard-main/src/examples/Sidenav/index.vue"],"sourcesContent":["<script setup>\nimport { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport SidenavList from \"./SidenavList.vue\";\nimport logo from \"@/assets/img/logo-ct-dark.png\";\nimport logoWhite from \"@/assets/img/logo-ct.png\";\n\nconst store = useStore();\nconst isRTL = computed(() => store.state.isRTL);\nconst layout = computed(() => store.state.layout);\nconst sidebarType = computed(() => store.state.sidebarType);\nconst darkMode = computed(() => store.state.darkMode);\n</script>\n<template>\n  <div\n    v-show=\"layout === 'default'\"\n    class=\"min-height-300 position-absolute w-100\"\n    :class=\"`${darkMode ? 'bg-transparent' : 'bg-info'}`\"\n  />\n\n  <aside\n    class=\"my-3 overflow-auto border-0 sidenav navbar navbar-vertical navbar-expand-xs border-radius-xl\"\n    :class=\"`${isRTL ? 'me-3 rotate-caret fixed-end' : 'fixed-start ms-3'}    \n      ${\n        layout === 'landing' ? 'bg-transparent shadow-none' : ' '\n      } ${sidebarType}`\"\n    id=\"sidenav-main\"\n  >\n    <div class=\"sidenav-header\">\n      <i\n        class=\"top-0 p-3 cursor-pointer fas fa-times text-secondary opacity-5 position-absolute end-0 d-none d-xl-none\"\n        aria-hidden=\"true\"\n        id=\"iconSidenav\"\n      ></i>\n\n      <router-link class=\"m-0 navbar-brand\" to=\"/\">\n        <img\n          :src=\"darkMode || sidebarType === 'bg-default' ? logoWhite : logo\"\n          class=\"navbar-brand-img h-100\"\n          alt=\"main_logo\"\n        />\n\n        <span class=\"ms-2 font-weight-bold me-2\">Argon Dashboard 2</span>\n      </router-link>\n    </div>\n\n    <hr class=\"mt-0 horizontal dark\" />\n\n    <sidenav-list />\n  </aside>\n</template>\n"],"mappings":"AACA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,WAAW,MAAM,mBAAmB;AAC3C,OAAOC,IAAI,MAAM,+BAA+B;AAChD,OAAOC,SAAS,MAAM,0BAA0B;;;;;;;IAEhD,MAAMC,KAAK,GAAGJ,QAAQ,CAAC,CAAC;IACxB,MAAMK,KAAK,GAAGN,QAAQ,CAAC,MAAMK,KAAK,CAACE,KAAK,CAACD,KAAK,CAAC;IAC/C,MAAME,MAAM,GAAGR,QAAQ,CAAC,MAAMK,KAAK,CAACE,KAAK,CAACC,MAAM,CAAC;IACjD,MAAMC,WAAW,GAAGT,QAAQ,CAAC,MAAMK,KAAK,CAACE,KAAK,CAACE,WAAW,CAAC;IAC3D,MAAMC,QAAQ,GAAGV,QAAQ,CAAC,MAAMK,KAAK,CAACE,KAAK,CAACG,QAAQ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}